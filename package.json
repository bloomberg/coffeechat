{
  "name": "coffeechat",
  "version": "0.0.0",
  "description": "An app for helping mentors and mentees connect and schedule time to meet for coffee or any comparable beverage.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev --trace-warnings --no-deps --respawn --no-notify --ignore-watch '.next' --project tsconfig.server.json src/server/index.ts",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --quiet --fix",
    "eslint:deadcode": "eslint . -c .eslintrc.deadcode.yml",
    "stylelint": "stylelint '**/*.tsx'",
    "stylelint:fix": "stylelint '**/*.tsx' --fix",
    "prettier:write": "prettier --write .",
    "prettier:verify": "prettier --check .",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "postinstall": "prisma generate",
    "gql:generate-ts-for-queries": "graphql-codegen --config codegen-queries.yml",
    "gql:update-schema-gql": "graphql-codegen --config codegen-get-schema.yml",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:serve": "serve .out -l 6006",
    "verify": "npm run prettier:verify && npm run eslint:deadcode && npm run stylelint && npm run build && npm test && npm run storybook:build",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloomberg/coffeechat.git"
  },
  "keywords": [
    "mentor",
    "mentee",
    "coffee",
    "chat"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bloomberg/coffeechat/issues"
  },
  "homepage": "https://github.com/bloomberg/coffeechat#readme",
